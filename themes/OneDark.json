{
  "$schema": "vscode://schemas/color-theme",
  "name": "OneDark++",
  "type": "dark",
  "colors": {
    // Editor
    "focusBorder": "#6190ee",
    "editorCursor.foreground": "#6190ee",
    "editor.background": "#272b33",
    "editor.foreground": "#abb2c1",
    "editor.lineHighlightBorder": "#2c323c",
    "editor.lineHighlightBackground": "#2c323c",
    "editor.inactiveSelectionBackground": "#3a3d41",
    "editorIndentGuide.background": "#404040",
    "editor.selectionHighlightBackground": "#add6ff26",
    "editor.selectionBackground": "#6190ee46",
    "editorLineNumber.foreground": "#4a5364",
    "editor.findMatchBackground": "#6190ee7a",
    "editor.findMatchHighlightBackground": "#6190ee10",
    "editorLink.activeForeground": "#6190ee",
    // Diff editor
    "diffEditor.insertedTextBackground": "#73c99019",
    "diffEditor.removedTextBackground": "#ff634719",
    // Sidebar & Activitybar
    "sideBar.foreground": "#fff",
    "sideBar.background": "#20242a",
    "activityBar.foreground": "#fff",
    "activityBar.background": "#20242a",
    "activityBarBadge.background": "#6190ee",
    "activityBarBadge.foreground": "#fff",
    // Tabs
    "tab.border": "#20242a",
    "editorGroup.dropBackground": "#23262d",
    "tab.activeBackground": "#272b33",
    "tab.activeForeground": "#fff",
    "tab.inactiveBackground": "#20242a",
    "editorGroup.border": "#fff",
    "editorGroupHeader.tabsBackground": "#20242a",
    // File explorer list
    "list.hoverBackground": "#272b33",
    "list.inactiveSelectionBackground": "#6190ee30",
    "list.activeSelectionBackground": "#6190ee50",
    // Input controls
    "input.background": "#272b33",
    "dropdown.background": "#272b33",
    // Panels & Widgets
    "panel.background": "#20242a",
    "panel.border": "#20242a",
    "editorWidget.background": "#20242a",
    // Statusbar
    "statusBar.foreground": "#fff",
    "statusBar.background": "#20242a",
    "statusBar.debuggingBackground": "#6190ee",
    "statusBar.noFolderBackground": "#fff",
    "statusBarItem.remoteForeground": "#fff",
    "statusBarItem.remoteBackground": "#6190ee",
    // Title Bar
    "titleBar.activeBackground": "#20242a",
    "titleBar.activeForeground": "#abb2c1",
    "titleBar.inactiveBackground": "#20242a",
    "titleBar.inactiveForeground": "#abb2c1",
    // Notification
    "notifications.background": "#20242a",
    "notifications.foreground": "#fff",
    // Menues
    "menu.background": "#272b33",
    "menu.foreground": "#abb2c1",
    "menubar.selectionBackground": "#20242a",
    "menubar.selectionForeground": "#fff",
    // Breadcrumb
    "breadcrumb.foreground": "#abb2c1",
    "breadcrumb.focusForeground": "#fff",
    "breadcrumb.activeSelectionForeground": "#fff",
    "breadcrumbPicker.background": "#272b33",
    // Debug toolbar
    "debugToolBar.background": "#272b33",
    // Git decorations
    "gitDecoration.modifiedResourceForeground": "#d19a66",
    "gitDecoration.deletedResourceForeground": "#e16971",
    "gitDecoration.untrackedResourceForeground": "#96c474",
    "gitDecoration.ignoredResourceForeground": "#5c6370",
    "gitDecoration.conflictingResourceForeground": "#c776df",
    // Terminal
    "terminal.ansiBlack": "#000",
    "terminal.ansiBlue": "#5eadf2",
    "terminal.ansiCyan": "#56b6c2",
    "terminal.ansiGreen": "#96c474",
    "terminal.ansiMagenta": "#c776df",
    "terminal.ansiRed": "#e16971",
    "terminal.ansiWhite": "#fff",
    "terminal.ansiYellow": "#eedd92",
    "terminal.ansiBrightBlack": "#000",
    "terminal.ansiBrightBlue": "#5eadf2",
    "terminal.ansiBrightCyan": "#56b6c2",
    "terminal.ansiBrightGreen": "#96c474",
    "terminal.ansiBrightMagenta": "#c776df",
    "terminal.ansiBrightRed": "#e16971",
    "terminal.ansiBrightWhite": "#fff",
    "terminal.ansiBrightYellow": "#eedd92",
    // Badge
    "badge.background": "#6190ee",
    "badge.foreground": "#fff"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#96c474",
        "fontStyle": ""
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#d19a66",
        "fontStyle": ""
      }
    },
    {
      "name": "User-defined constant & Language variable",
      "scope": [
        "constant.character",
        "variable.language"
      ],
      "settings": {
        "foreground": "#c776df",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.readwrite",
        "variable.readwrite.other.block",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#e16971",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "punctuation.definition.keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#c776df",
        "fontStyle": ""
      }
    },
    {
      "name": "Separator",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.destructuring",
        "punctuation.separator.annotation.python",
        "punctuation.separator.dict.python"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": ""
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "entity.other.inherited-class",
        "storage.identifier",
        "support.class",
        "support.other.namespace",
        "support.type"
      ],
      "settings": {
        "foreground": "#eedd92",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "meta.function-call",
        "meta.method-call",
        "support.function.construct.output",
        "entity.name.function.stylus",
        "support.function.filter.css",
        "support.function.misc"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function definition",
      "scope": [
        "meta.definition.function",
        "meta.definition.method",
        "entity.name.function.stylus",
        "support.function.filter.css"
      ],
      "settings": {
        "foreground": "#5eadf2",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation & Variable",
      "scope": [
        "punctuation.accessor",
        "punctuation.definition.arguments",
        "punctuation.separator.delimiter",
        "punctuation.terminator",
        "punctuation.section",
        "constant.name.attribute.tag",
        "punctuation.definition.begin",
        "punctuation.definition.end",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        // Variable
        "variable.other",
        "support.variable",
        "meta.function-call.arguments",
        // Library constant
        "support.constant"
      ],
      "settings": {
        "foreground": "#abb2c1",
        "fontStyle": ""
      }
    },
    {
      "name": "Builtin Functions",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#96c474",
        "fontStyle": ""
      }
    },
    {
      "name": "Template expression",
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#eedd92",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx"
      ],
      "settings": {
        "foreground": "#e16971",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": [
        "entity.name.tag.class",
        "entity.name.tag.id",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#d19a66",
        "fontStyle": ""
      }
    },
    {
      "name": "Attribute Class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#eedd92",
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Property Name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.postcss",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#abb2bf",
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.postcss",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#96c474",
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.postcss",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#abb2c1",
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.postcss.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#e5c07b",
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": [
        "unit.css",
        "unit.postcss",
        "unit.scss",
        "unit.less",
        "unit.sass"
      ],
      "settings": {
        "foreground": "#c776df",
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.postcss",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": ""
      }
    },
    {
      "name": "JSON Property Name",
      "scope": [
        "support.type.property-name",
        "meta.object-literal.key",
        "variable.object.property"
      ],
      "settings": {
        "foreground": "#e16971",
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#e16971",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Link",
      "scope": [
        "string.detected-link",
        "linkForeground"
      ],
      "settings": {
        "foreground": "#eedd92",
        "fontStyle": " italic underline"
      }
    },
    {
      "name": "Diff header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#e16971",
        "fontStyle": ""
      }
    },
    {
      "name": "Diff inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#96c474",
        "fontStyle": ""
      }
    },
    {
      "name": "Diff changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": ""
      }
    }
  ]
}
